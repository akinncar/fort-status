{
  "expo": {
    "name": "FortStatus",
    "slug": "fort-status",
    "owner": "akinncar",
    "description": "App to search and find your Fortnite games stats.",
    "githubUrl": "https://github.com/akinncar/fort-status",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6100FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
